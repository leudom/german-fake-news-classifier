# Project specific variables to execute the ML pipeline
PROJECT_DIR=<absolute path to local project directory>
KAGGLE_USERNAME=<your kaggle username>
KAGGLE_KEY=<your kaggle key>
MODEL_PATH=<${PROJECT_DIR}/your_model_path>

# Deployment specific variables
AZURE_CONFIG_PATH=${PROJECT_DIR}/.azureml/config.json
MODEL_NAME=<your model name>
