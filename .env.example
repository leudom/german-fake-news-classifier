ENV_DEPENDENCIES=${PROJECT_DIR}/.azureml/environment.yml
SRC_DIR=<your source_dir>
ENTRY_SCRIPT=<your entry script>
DEPLOYMENT_NAME=<your deployment name>
LOCAL_PORT=<your local port>
CPU_CORES=<number of cores>
MEMORY_GB=<number of memory>
